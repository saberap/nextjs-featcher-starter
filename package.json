{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "init": "cd scripts/ && init.bat",
    "prepare": "husky install",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "pre-commit": "next lint && next build",
    "create:feature": "sh ./scripts/create-feature.sh",
    "create:api": "sh ./scripts/create-api.sh",
    "create:component": "sh ./scripts/create-component.sh",
    "create:config": "sh ./scripts/create-config.sh",
    "create:constant": "sh ./scripts/create-constant.sh",
    "create:context": "sh ./scripts/create-context.sh",
    "create:hook": "sh ./scripts/create-hook.sh",
    "create:type": "sh ./scripts/create-type.sh",
    "create:utility": "sh ./scripts/create-utility.sh",
    "create:slice": "sh ./scripts/create-slice.sh"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "husky": "^8.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "typescript": "^5"
  }
}
